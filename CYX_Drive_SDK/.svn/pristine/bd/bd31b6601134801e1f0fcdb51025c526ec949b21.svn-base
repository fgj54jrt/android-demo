package com.cwits.cyx_drive_sdk.bean;

import com.cwits.cyx_drive_sdk.libDriveBh.SensorData;

/**
 * 行驶事件数据对象
 * @author lxh
 *
 */
public class DriveEvent {
	private String ter_id;				//设备内部ID
	private Position position;			//位置点
	private int type;					//事件类型 6 急加速7急转弯8 急减速
	private SensorData detial;			//事件详情数据(传感器相关)
	public String getTer_id() {
		return ter_id;
	}
	
	
	public void setTer_id(String ter_id) {
		this.ter_id = ter_id;
	}
	public Position getPosition() {
		return position;
	}
	public void setPosition(Position position) {
		this.position = position;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public SensorData getDetial() {
		return detial;
	}
	public void setDetial(SensorData detial) {
		this.detial = detial;
	}
	
}
