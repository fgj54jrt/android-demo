<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/regist_bg_color" >
    <include layout="@layout/cyx_title" android:id="@+id/title"/>
    <include 
        android:id="@+id/hint" 
        android:layout_width="match_parent" 
        android:layout_height="wrap_content" 
        android:layout_below="@id/title" 
        layout="@layout/cyx_vip_hint"
        android:layout_marginTop="15dp"/>
	
	<include android:id="@+id/car_fortify_layout"
	    layout="@layout/cyx_carfortify"
	     android:layout_width="match_parent" 
        android:layout_height="wrap_content" 
        android:layout_below="@+id/hint"
        android:visibility="gone"/>
	 <Button 
	    android:layout_alignParentBottom="true"
	    android:id="@+id/btn_vip"
	    android:layout_width="wrap_content"
	    android:layout_height="50dp"
	    android:text="@string/to_be_vip"
	    android:textColor="@color/white"
	    android:textSize="18sp"
	    android:layout_centerHorizontal="true"
	    android:layout_marginLeft="80dp"
	    android:layout_marginRight="80dp"
	    android:layout_marginBottom="50dp"
	    android:layout_marginTop="30dp"
	    android:background="@drawable/btn_resendcode_bg"
	    android:layout_below="@+id/car_fortify_layout"/>
</RelativeLayout>
