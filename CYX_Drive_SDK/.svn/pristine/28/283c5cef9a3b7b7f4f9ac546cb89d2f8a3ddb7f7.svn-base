package com.cwits.cyx_drive_sdk.bean;

import java.io.Serializable;

public class PositionResult implements Serializable{
	private static final long serialVersionUID = 1L;
	private String ter_id;  //设备编号
	private String time;    //数据上传时间  yyyy-MM-dd mm:hh:ss
	private double lon;		//经度
	private double lat;		//纬度
	private float speed; 	//速度
	private double height;	//海拔
	private float direction;//方向
	private int type;		//：0 定位点，1补偿点 6 急加速，7急转弯，8 急减速
	private String stroke;	//行程ID
	private int state ;		// 定位状态 0无效 1 有效
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public double getLon() {
		return lon;
	}
	public void setLon(double lon) {
		this.lon = lon;
	}
	public double getLat() {
		return lat;
	}
	public void setLat(double lat) {
		this.lat = lat;
	}
	public float getSpeed() {
		return speed;
	}
	public void setSpeed(float speed) {
		this.speed = speed;
	}
	public double getHeight() {
		return height;
	}
	public void setHeight(double height) {
		this.height = height;
	}
	public float getDirection() {
		return direction;
	}
	public void setDirection(float direction) {
		this.direction = direction;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public String getTer_id() {
		return ter_id;
	}
	public void setTer_id(String ter_id) {
		this.ter_id = ter_id;
	}
	public String getStroke() {
		return stroke;
	}
	public void setStroke(String stroke) {
		this.stroke = stroke;
	}
	public int getState() {
		return state;
	}
	public void setState(int state) {
		this.state = state;
	}
	
}
