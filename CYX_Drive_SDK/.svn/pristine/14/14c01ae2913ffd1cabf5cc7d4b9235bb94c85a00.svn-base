<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:angle="270"
                android:centerColor="@color/whb_grey"
                android:centerY="0.75"
                android:endColor="@color/whb_grey"
                android:startColor="@color/whb_grey" />

            <stroke
                android:dashGap="1dp"
                android:dashWidth="1dp"
                android:width="15dp"
                android:color="#00000000" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerColor="@color/whb_black"
                    android:centerY="0.75"
                    android:endColor="@color/whb_black"
                    android:startColor="@color/whb_black" />

                <stroke
                    android:dashGap="1dp"
                    android:dashWidth="1dp"
                    android:width="15dp"
                    android:color="#00000000" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:angle="270"
                    android:centerColor="#34b67a"
                    android:centerY="0.75"                    android:endColor="#34b67a"
                    android:startColor="#34b67a" />

                <stroke
                    android:dashGap="1dp"
                    android:dashWidth="1dp"
                    android:width="15dp"
                    android:color="#00000000" />
            </shape>
        </clip>
    </item>

</layer-list>